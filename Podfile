# platform :ios, '9.0'

target 'ReCrowd' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for ReCrowd

  target 'ReCrowdTests' do
    inherit! :search_paths
    # Pods for testing
  end
  
  # Pods for Firebase
    pod 'Firebase/Auth'
	pod 'Firebase/Core'

  # Pods for Facebook
  	pod 'Bolts'
  	pod 'FBSDKCoreKit'
  	pod 'FBSDKLoginKit'
    
  # Pods for UI
    pod 'Font-Awesome-Swift', '~> 1.7.2'
end
